
ACCOUNTS_DONT_DELETE_REGEX="^(root|hs)"
GROUP_DONT_DELETE_REGEX="^(root|hs)"
KMC_RM=0

# Fix ownersip.
chgrp -R hs /home/hs

# fix perms
chmod -R go+rX /etc/passwd /etc/group /etc/init /usr/lib/python3.9/site-packages/kcore /usr/lib/python3.9/site-packages/ktools

# lib visibility
ln -s /usr/lib/python3.9/site-packages/ktools/ratelimiter.py /usr/local/bin/ratelimiter
ln -s /usr/lib/python3.9/site-packages/ktools/pb-push.sh /usr/local/bin/pb-push
